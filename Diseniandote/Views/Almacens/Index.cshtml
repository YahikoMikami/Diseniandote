@model IEnumerable<Diseniandote.Models.Almacen>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Material.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Material.nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idAlmacen }) |
            @Html.ActionLink("Details", "Details", new { id=item.idAlmacen }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idAlmacen })
        </td>
    </tr>
}

</table>
